#include <stdio.h>

struct book {
    char title[100];
    char author[100];
    float price;
};

int main() {
  
    struct book myBook;

    strcpy(myBook.title, "The Great Gatsby");
    strcpy(myBook.author, "F. Scott Fitzgerald");
    myBook.price = 10.99;
    printf("Title: %s\n", myBook.title);
    printf("Author: %s\n", myBook.author);
    printf("Price: $%.2f\n", myBook.price);

    return 0;
}
